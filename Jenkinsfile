pipeline {
    agent any
    tools {
            maven 'Maven 3.3.9'
            jdk 'jdk8'
        }
        stages {
            stage("Dit SALAM JENKINS") {
                steps {
                    echo "dede"
                }
            }
            stage("Run Unit Test") {
                steps {
                    sh 'mvn test'
                }
            }
            stage("DIT BISLEHMA") {
                steps {
                    echo "dededede"
                }
            }
        }
}